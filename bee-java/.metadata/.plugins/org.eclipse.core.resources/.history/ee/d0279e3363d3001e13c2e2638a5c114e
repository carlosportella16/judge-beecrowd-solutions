package bee1010;

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double total = 0;
		for (int i = 0; i <=1; i++) {
			 sc.next();
			int a = sc.nextInt();
			int b = sc.nextInt();
			double c = sc.nextDouble();
			
			 total += (b*c);
		}
		
		System.out.printf("TOTAL = R$ %.2f\n", total);
		sc.close();
	}

}
